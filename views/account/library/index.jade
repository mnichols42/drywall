extends ../../../layouts/account

block head
  title Library

block neck
  link(rel='stylesheet', href='/views/account/library/index.min.css?#{cacheBreaker}')

block feet
  script(src='http://crypto-js.googlecode.com/svn/tags/3.1.2/build/rollups/sha1.js')
  script(src='https://sdk.amazonaws.com/js/aws-sdk-2.1.10.min.js')
  script(src='/views/account/library/index.min.js?#{cacheBreaker}')

block body
  div.row
    div.col-sm-12
      div.page-header
        h1 Library
      #uploads
        for upload in uploads
          div.row
            a(href='/account/file/'+upload)= upload
